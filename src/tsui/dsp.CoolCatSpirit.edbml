<!DOCTYPE edbml>
<script>
	<?input name="coolcat" type="dsp.CoolCatModel"?>
	@id = 'cool-cat-' + coolcat.$instanceid
	@class = 'cool-cat'
	@width = coolcat.width
	@height = coolcat.height
	@src = '../assets/retardio.png'
	@style = this._getTransform(coolcat.transform)

	<img @id @class @width @height @src @style />
</script>
